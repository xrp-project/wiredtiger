    "version": "0.2.0",
    "configurations": [
        {
            "name": "wiredtiger xdp launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/xdp/main.out",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description":  "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "wiredtiger ycsb c - read-only",
            "type": "cppdbg",
            "request": "launch",
            "program": "/mydata/XRP/My-YCSB/build/run_wt",
            "args": [
                "/mydata/XRP/My-YCSB/wiredtiger/config/ycsb_c.yaml",
            ],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description":  "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}